<?xml version="1.0"?>
<openerp>
    <data>



        <record id="gps_base_view_users_form_simple_modif" model="ir.ui.view">
            <field name="name">gps_base.view_users_form_simple_modif</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form_simple_modif"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='tz']" position="after">
                    <field name="coords_format"/>
                </xpath>
            </field>
        </record>


        <record id="gps_base_view_users_form" model="ir.ui.view">
            <field name="name">gps_base.view_users_form</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='tz']" position="after">
                    <field name="coords_format"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>

